FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsR;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    top
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    electrode_exposed
    {
        type            zeroGradient;
    }
    electrode_grounded
    {
        type            zeroGradient;
    }
    air_to_dielectric1
    {
        type            fixedValue;
        value           uniform 1;
    }
}



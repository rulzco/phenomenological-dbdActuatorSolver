FoamFile
{
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  2;

method          scotch;

simpleCoeffs
{
    n           (2 2 1);
}

hierarchicalCoeffs
{
    n           (2 2 1);
    order       xyz;
}

